{
    "ecmaFeatures": {
        "ecmaVersion": 6,
        "jsx" : true
    },
    "env" : {
        "browser" : true,
        "node":true,
        "jquery" :false,
        "mocha":true,
        "es6": true
    },
    "plugins":["react"],
    "extends":["airbnb", "eslint:recommended", "plugin:react/recommended"],
    "rules": {
        "strict": [2,"global"],
        "jsx-quotes": [2,"prefer-single"],
        "quotes": [2,"single"],
        "import/no-dynamic-require": 0,
        "react/jsx-indent": [2, 4],
        "import/extensions": [2,{ "js": "never", "jsx": "always" }]
    }
}